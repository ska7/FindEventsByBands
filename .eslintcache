n              // fontSize: \"100%\",\n              textAlign: \"center\",\n              [theme.breakpoints.down(\"xs\")]: {\n                // fontSize: \"2px\",\n                fontWeight: \"900\",\n              },\n              [theme.breakpoints.up(\"md\")]: {\n                fontSize: \"23px\",\n              },\n              [theme.breakpoints.up(\"lg\")]: {\n                fontSize: \"27px\",\n              },\n            },\n            date: {\n              fontWeight: \"900\",\n              fontSize: \"25px\",\n            },\n            favoriteBtn: {\n              width: \"10%\",\n              // border: \"1px solid red\",\n              margin: 0,\n              padding: 0,\n            },\n          }\n        : // Item Of List Styles\n          {\n            mainContainer: {\n              display: \"flex\",\n              flexDirection: \"row\",\n              justifyContent: \"center\",\n              alignItems: \"center\",\n              width: \"100%\",\n            },\n            checkbox: {\n              \"&:hover\": {\n                backgroundColor: \"rgba(255,255,255, 0.7)\",\n                borderRadius: \"50%\",\n              },\n            },\n            eventTime: {\n              color: \"rgb(220,220,220)\",\n              [theme.breakpoints.down(\"xs\")]: {\n                fontSize: \"15px\",\n              },\n            },\n            link: {\n              ...theme.links,\n            },\n            eventDate: {\n              width: \"10%\",\n              padding: \"0\",\n              margin: \"0\",\n            },\n            location: {\n              fontWeight: \"300\",\n              color: \"rgb(220,220,220)\",\n              textAlign: \"center\",\n              [theme.breakpoints.down(\"xs\")]: {\n                padding: \"0px 20px\",\n              },\n            },\n            eventName: {\n              width: \"80%\",\n              display: \"flex\",\n              flexDirection: \"column\",\n              justifyContent: \"center\",\n              alignItems: \"center\",\n            },\n            name: {\n              fontWeight: \"700\",\n              color: \"white\",\n              textAlign: \"center\",\n              [theme.breakpoints.down(\"xs\")]: {\n                padding: \"0px 20px\",\n              },\n            },\n            date: {\n              fontWeight: \"900\",\n            },\n            favoriteBtn: {\n              // height: \"40px\",\n              // width: \"40px\",\n              [theme.breakpoints.down(\"xs\")]: {\n                margin: 0,\n              },\n            },\n          }\n    )\n  );\n};\n\nconst formatDate = (date) => {\n  // Date should come as yyyy-mm-dd\n\n  let [year, month, day] = date.split(\"-\");\n  if (month.includes(\"0\")) month = month.replace(\"0\", \"\");\n\n  const months = [\n    \"#\",\n    \"JAN\",\n    \"FEB\",\n    \"MAR\",\n    \"APR\",\n    \"MAY\",\n    \"JUN\",\n    \"JUL\",\n    \"AUG\",\n    \"SEP\",\n    \"OCT\",\n    \"NOV\",\n    \"DEC\",\n  ];\n\n  return `${months[month]} ${day}`;\n};\n\nconst formatTime = (time) => {\n  // Time should come as hh:mm:ss\n\n  const [hours, minutes] = time.split(\":\");\n\n  return `${hours}:${minutes}`;\n};\n\n// const formatName = (name) => {\n//   return name.length > 40 ? `${name.slice(0, 40)}...` : name;\n// };\n\nexport const EventGeneralInformation = ({ event, isStandAlone }) => {\n  const classes = useStyles(isStandAlone)();\n\n  const { favorites, setFavorites } = useContext(FavoritesContext);\n  const xsScreen = useMediaQuery(\"(max-width: 450px)\");\n\n  const [isChecked, setChecked] = useState(false);\n\n  useEffect(() => {\n    setChecked(checkIfSaved(event.id, favorites));\n  }, [favorites]);\n\n  return (\n    <Container className={classes.mainContainer} id=\"event-general-information\">\n      <Container className={classes.eventDate}>\n        <Typography color=\"secondary\" className={classes.date}>\n          {formatDate(event.start.date)}\n        </Typography>\n        {event.start.time && (\n          <Typography className={classes.eventTime}>\n            {formatTime(event.start.time)}\n          </Typography>\n        )}\n      </Container>\n      <Container className={classes.eventName}>\n        <Typography className={classes.name}>{event.displayName}</Typography>\n        <Typography className={classes.location}>\n          {event.location.city}\n        </Typography>\n      </Container>\n      <FormControlLabel\n        className={classes.favoriteBtn}\n        control={\n          <Checkbox\n            icon={\n              <FavoriteBorder\n                fontSize={isStandAlone ? \"large\" : \"medium\"}\n                color=\"secondary\"\n              />\n            }\n            onClick={() => updateFavorites(event, favorites, setFavorites)}\n            checked={isChecked}\n            checkedIcon={\n              <Favorite fontSize={isStandAlone ? \"large\" : \"medium\"} />\n            }\n            name=\"checkedH\"\n          />\n        }\n      />\n    </Container>\n  );\n};\n","/Users/nickaralin/Documents/Projects/events-finder/src/components/Search/MatchedResultsList.js",[],"/Users/nickaralin/Documents/Projects/events-finder/src/components/Event/EventLineUp.js",["145","146"],"import React, { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\n\nimport {\n  Button,\n  Container,\n  createStyles,\n  TableBody,\n  TableCell,\n  TableContainer,\n  Typography,\n  useMediaQuery,\n} from \"@material-ui/core\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport { IconButton, Link as MaterialLink, Collapse } from \"@material-ui/core\";\nimport ExpandMoreIcon from \"@material-ui/icons/ExpandMore\";\nimport ExpandLessIcon from \"@material-ui/icons/ExpandLess\";\nimport TableRow from \"@material-ui/core/TableRow\";\n\nconst useStyles = (isStandAlone) => {\n  return makeStyles((theme) =>\n    createStyles(\n     